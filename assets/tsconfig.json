{
  "compilerOptions": {
    "strict": true,
    "target": "es2017",
    "moduleResolution": "node",
    "skipLibCheck": true,
    "types": [
      "node"
    ],
    "baseUrl": ".",
    "paths": {
      "$lib": [
        "svelte"
      ],
      "$lib/*": [
        "svelte/*"
      ]
    },
    "lib": [
      "es2017",
      "dom"
    ]
  }
}
